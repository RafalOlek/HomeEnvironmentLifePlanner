@page "/account/create"
@inject HttpClient http
@inject NavigationManager uriHelper

<FormACC ButtonText="@Properties.Dictionary.sharedCreate" account="@account" Submit="@CreateAccount" />
@code {
    Account account = new Account();
    async Task CreateAccount()
    {
        await http.PostAsJsonAsync("api/config/account", account);
        uriHelper.NavigateTo("config");
    }
}